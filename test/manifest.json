{
    "manifest_version": 3,
    "name": "FoE Data Standalone Test",
    "version": "1.0",
    "description": "Test extension for standalone MainParser",
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "host_permissions": [
        "*://*.forgeofempires.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.forgeofempires.com/game/index*"
            ],
            "js": [
                "loader.js"
            ],
            "run_at": "document_start"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "build_mainparser.js"
            ],
            "matches": [
                "*://*.forgeofempires.com/*"
            ]
        }
    ]
}